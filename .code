#include <Servo.h>

int bumper = 8;
Servo rightMotor;
Servo leftMotor;

void setup() {
rightMotor.attach(9);
leftMotor.attach(10);
pinMode(bumper,INPUT);
}

void loop() {
    for (int i = 110; i<=180; i++)
    {
      rightMotor.write(i);
      leftMotor.write(i);
      delay(15);
   }
   for (int j = 180; j>=110; j--)
   {
     rightMotor.write(j);
     leftMotor.write(j);
     delay(15);
   }
   if (digitalRead(bumper)==HIGH){
     
   }
 }
